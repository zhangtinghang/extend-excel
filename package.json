{
  "name": "extend-excel",
  "version": "0.0.3",
  "description": "A export and inport excel funciton.",
  "type": "module",
  "main": "dist/index.min.mjs",
  "module": "dist/index.min.mjs",
  "umd:main": "dist/index.min.umd.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "umd": "./dist/index.min.umd.js",
      "import": "./dist/index.min.mjs",
      "require": "./dist/index.min.cjs",
      "default": "./dist/index.min.mjs"
    }
  },
  "scripts": {
    "prepublish": "npm run build",
    "prepare": "husky install",
    "test": "jest",
    "test:clear": "jest --clearCache",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "lint": "eslint --cache --ext .ts src/**",
    "format": "prettier --write --cache --parser typescript \"src/**/*.ts\"",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhangtinghang/extend-excel.git"
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "keywords": [
    "export",
    "import",
    "excel",
    "JSON to xslx",
    "import excel",
    "export excel"
  ],
  "author": "zhang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zhangtinghang/extend-excel/issues"
  },
  "homepage": "https://github.com/zhangtinghang/extend-excel#readme",
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-typescript": "^7.23.2",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.0.2",
    "prettier": "3.0.3",
    "rollup": "^4.3.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "*"
  },
  "dependencies": {
    "file-saver": "^2.0.5",
    "lodash": "^4.17.21",
    "xlsx": "^0.18.5"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-typescript"
    ]
  }
}
